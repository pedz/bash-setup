
[[ -n "${debug}" ]] && echo ./env/p51

if [[ -n "$PS1" && "$TERM" == xterm-color ]]; then
    stty erase '^?'
    oldTerm=$TERM
    TERM=xterm
fi

export MANPATH=\
/usr/man:\
/usr/local/man:\
/usr/local/ssl/man:\
/usr/local/share/man

# path to local copy of retain tools on truth
export HPATH=/usr/local/retain/

# move .Xauthority file out of dfs
# export XAUTHORITY=/home/pedzan/.Xauthority

source $HOME/.bash/env/aix
