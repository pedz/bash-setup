# -*- mode: shell-script -*-

[[ -n "${debug}" ]] && echo ./env/p51

if [[ -n "$PS1" && "$TERM" == xterm-color ]]; then
    stty erase '^?'
    oldTerm=$TERM
    TERM=xterm
fi

cmvc_base=/gsa/ausgsa/projects/a/aix_tools/bin/cmvc
export MANPATH=\
/usr/man:\
/usr/local/man:\
/usr/local/ssl/man:\
/usr/local/share/man:\
/usr/local/pgsql/man

source $HOME/.bash/env/aix
