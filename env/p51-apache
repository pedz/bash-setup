# -*- mode: shell-script -*-

[[ -n "${debug}" ]] && echo ./env/p51-apache

if [[ -n "$PS1" && "$TERM" == xterm-color ]]; then
    stty erase '^?'
    oldTerm=$TERM
    TERM=xterm
fi

export MANPATH=\
/usr/man:\
/usr/local/man:\
/usr/local/ssl/man:\
/usr/local/share/man

export PGUSER=raptor
export PGDATABASE=raptor_production
